
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<!-- Mirrored from thunder-team.com/friend-finder/index-register.html by HTTrack Website Copier/3.x [XR&CO'2014], Mon, 27 Feb 2017 12:45:56 GMT -->
<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="This is social network html5 template available in themeforest......" />
    <meta name="keywords" content="Social Network, Social Media, Make Friends, Newsfeed, Profile Page" />
    <meta name="robots" content="index, follow" />
    <title>Gerard & Jenny</title>

    <!-- Stylesheets
    ================================================= -->
    <link rel="stylesheet" href="~/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/style.css" />
    <link rel="stylesheet" href="~/css/ionicons.min.css" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css" />

    <!--Favicon-->
    <link rel="shortcut icon" type="image/png" href="~/images/paintBrush.png" />
</head>
<body>

    <header id="header-inverse">
        <nav class="navbar navbar-default navbar-fixed-top menu">
            <div class="container">

                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <h1 class="text-white" style="margin-left:20px">Arts & Crafts Supplies</h1>
                </div>
            </div><!-- /.container -->
        </nav>
    </header>


    <div id="lp-register">
        <div class="container wrapper">
            <div class="row" style="margin-top:150px">
                <div class="col-sm-8">
                    <div class="intro-texts">
                        @*<h1 class="text-white">Arts & Crafts Supplies</h1>*@
                        @*<p>Friend Finder is a social network template that can be used to connect people. The template offers Landing pages, News Feed, Image/Video Feed, Chat Box, Timeline and lot more. <br /> <br />Why are you waiting for? Buy it now.</p>*@
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="reg-form-container">

                        <!--Registration Form Contents-->
                        <div class="tab-content">
                            <!--Login-->
                            <div class="tab-pane active" id="login">
                                <h3 style="color:white">Login</h3>
                                <p class="text-muted">Log into your account</p>

                                <!--Login Form-->
                                <form name="Login_form" action="/User/Index" method="post" id='Login_form'>
                                    <div class="row">
                                        <div class="form-group col-xs-12">
                                            <label for="my-email" class="sr-only">Username</label>
                                            <input id="my-email" class="form-control input-group-lg" type="text" name="username" title="Enter Username" placeholder="First name@artscraftsmedia.com" required />
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group col-xs-12">
                                            <label for="my-password" class="sr-only">Password</label>
                                            <input id="my-password" class="form-control input-group-lg" type="password" name="password" title="Enter password" placeholder="Password" required />
                                        </div>
                                    </div>
                                    <!--Login Form Ends-->
                                    <br />
                                    <div style="text-align:center">
                                        <button type="submit" class="btn btn-primary" style="vertical-align:middle;background-color:#f970a4">Login Now</button>
                                    </div>

                                    @if (ViewData["LoginError"] != null)
                                    {
                                        <p style="color:red">@ViewData["LoginError"]</p>
                                    }
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--preloader-->
    @*<div id="spinner-wrapper">
            <div class="spinner"></div>
        </div>*@

    <!-- Scripts
    ================================================= -->
    <script src="~/js/jquery-3.1.1.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/jquery.appear.min.js"></script>
    <script src="~/js/jquery.incremental-counter.js"></script>
    <script src="~/js/script.js"></script>

</body>

<!-- Mirrored from thunder-team.com/friend-finder/index-register.html by HTTrack Website Copier/3.x [XR&CO'2014], Mon, 27 Feb 2017 12:45:56 GMT -->
</html>


